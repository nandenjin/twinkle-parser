{
  "name": "twinkle-parser",
  "version": "0.4.16",
  "description": "Parse CSV from https://kdb.tsukuba.ac.jp to structured JSON.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "twinkle-parser": "./bin/twinkle-parser.js"
  },
  "scripts": {
    "start": "npm run parse",
    "parse": "node bin/twinkle-parser.js",
    "build": "tsc",
    "lint": "eslint --ext ts,js --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext ts,js --ignore-path .gitignore .",
    "test": "jest",
    "format": "prettier --write \"**/*.{ts,js,json,md,yml,yaml}\"",
    "prepare": "npm run build",
    "release": "shipjs prepare"
  },
  "author": "Kazumi Inada <hello@nandenjin.com>",
  "license": "MIT",
  "dependencies": {
    "consola": "^3.0.0",
    "csv-parse": "^5.3.0",
    "iconv-lite": "^0.6.2",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "@types/iconv": "3.0.2",
    "@types/jest": "29.5.5",
    "@types/node": "18.18.5",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-jest": "27.4.2",
    "jest": "29.7.0",
    "prettier": "3.0.3",
    "shipjs": "0.26.3",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  }
}
