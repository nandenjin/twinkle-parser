{
  "name": "twinkle-parser",
  "version": "0.4.13",
  "description": "Parse CSV from https://kdb.tsukuba.ac.jp to structured JSON.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "twinkle-parser": "./bin/twinkle-parser.js"
  },
  "scripts": {
    "start": "npm run parse",
    "parse": "node bin/twinkle-parser.js",
    "build": "tsc",
    "lint": "eslint --ext ts,js --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext ts,js --ignore-path .gitignore .",
    "test": "jest",
    "format": "prettier --write \"**/*.{ts,js,json,md,yml,yaml}\"",
    "prepare": "npm run build",
    "release": "shipjs prepare"
  },
  "author": "Kazumi Inada <hello@nandenjin.com>",
  "license": "MIT",
  "dependencies": {
    "consola": "^2.15.0",
    "csv-parse": "^5.3.0",
    "iconv-lite": "^0.6.2",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "@types/iconv": "3.0.1",
    "@types/jest": "29.4.0",
    "@types/node": "18.11.19",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-jest": "27.2.1",
    "jest": "29.4.1",
    "prettier": "2.8.3",
    "shipjs": "0.25.1",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
