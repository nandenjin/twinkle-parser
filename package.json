{
  "name": "twinkle-parser",
  "version": "0.4.7",
  "description": "Parse CSV from https://kdb.tsukuba.ac.jp to structured JSON.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "twinkle-parser": "./bin/twinkle-parser.js"
  },
  "scripts": {
    "start": "npm run parse",
    "parse": "node bin/twinkle-parser.js",
    "build": "tsc",
    "lint": "eslint --ext ts,js --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext ts,js --ignore-path .gitignore .",
    "test": "jest",
    "format": "prettier --write \"**/*.{ts,js,json,md,yml,yaml}\"",
    "prepare": "npm run build",
    "release": "shipjs prepare"
  },
  "author": "Kazumi Inada <hello@nandenjin.com>",
  "license": "MIT",
  "dependencies": {
    "consola": "^2.15.0",
    "csv-parse": "^4.4.5",
    "iconv-lite": "^0.6.2",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "@types/iconv": "^3.0.0",
    "@types/jest": "^27.0.1",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^26.0.0",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "shipjs": "0.24.4",
    "ts-jest": "^26.1.1",
    "typescript": "^4.0.3"
  }
}
